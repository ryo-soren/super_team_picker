    <div class="form-group m-3">
      <label for="image_url" class="my-2">Logo Url</label>
      <input name="image_url" type="url" class="form-control" id="image_url" placeholder="e.g. https://www.example.com/some.png" value="<%-cohort ? cohort.image_url : ""%>">
    </div>
    <div class="form-group m-3">
        <label for="team_name" class="my-2">Name</label>
        <input name="team_name" type="text" class="form-control" id="team_name" placeholder="Flying Flunks" value="<%-cohort ? cohort.team_name : ""%>">
    </div>
    <div class="form-group m-3">
        <label for="members" class="my-2">Members</label>
        <textarea name="members" type="text" class="form-control" id="members" placeholder="Jon Snow, Daenerys, Melisandre, Arya Stark" ><%-cohort ? cohort.members : ""%></textarea>
        <div class=" mt-3 d-flex flex-row-reverse">
            <button type="submit" class="btn btn-primary">submit</button>
            <% if (cohort) { %>
                <button type="submit" class="mx-3 btn btn-outline-primary">return</button>
            <% } %>
        </div>
        </form>
    </div>
